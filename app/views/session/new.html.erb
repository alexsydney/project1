<h4>Login page</h4>

<%= flash[:error] %>

<%= form_tag login_path do %>
  <%= label_tag :email %>
  <%= email_field_tag :email, nil %>

  <%= label_tag :password %>
  <%= password_field_tag :password, nil %>

  <%#= label_tag :confirm_password %>
  <%#= password_field_tag :confirm_password, nil %>

  <%= submit_tag 'Login' %>

<% end %>
